                                                                                                                                                                              #Generating Random Values
#Code to find 5 random numbers
import random

for i in range (5):
    print(random.random())



#code to pick random name
import random

members = ["waj","Maj","Ayesha", "Adam"]
leader = random.choice(members)
print (leader)


#Code for dice roll game
import random

class Dice:
    def roll(self):
        first = random.randint(1,6)
        second = random.randint(1,6)
        return first,second

dice = Dice()
print(dice.roll())
